<?xml version='1.0' encoding='utf-8'?>
<widget id="com.ancebetaapp.com" version="1.0.53" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>Ance Beta 0.9</name>
    <description>
        Ance news dispatcher beta version
    </description>
    <author email="bluemix@afbnet.it" href="http://www.afbnet.it">
        AFB Net - A Vargroup company
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-navigation href="*" />
    <allow-intent href="*" />
    <js-module name="BrowserPush" src="www/browser/push.js">
        <clobbers target="PushNotification" />
    </js-module>
    <asset src="src/browser/ServiceWorker.js" target="ServiceWorker.js" />
    <asset src="src/browser/manifest.json" target="manifest.json" />
    <hook src="hooks/browser/updateManifest.js" type="after_prepare" />
    <preference name="android-build-tool" value="gradle" />
</widget>
