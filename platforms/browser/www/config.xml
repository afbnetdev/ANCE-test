<?xml version='1.0' encoding='utf-8'?>
<widget id="com.ance.com.anceapp.com" version="1.4.5" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <feature name="Device">
        <param name="browser-package" value="Device" />
    </feature>
    <feature name="Camera">
        <param name="browser-package" value="Camera" />
    </feature>
    <feature name="Badge">
        <param name="browser-package" value="Badge" />
    </feature>
    <name>ANCE official App</name>
    <description>
        Ance news dispatcher
    </description>
    <author email="l.lombardi@afbnet.it" href="http://www.afbnet.it">
        AFB Net - A Vargroup company
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-navigation href="*" />
    <allow-intent href="*" />
    <edit-config file="*-Info.plist" mode="merge" target="NSCalendarsUsageDescription">
        <string>This app may use your calendar in order to schedule events related to ANCE Activity only</string>
    </edit-config>
    <preference name="android-build-tool" value="gradle" />
    <preference name="phonegap-version" value="cli-8.0.0" />
</widget>
